<Page x:Class="HCI_Tim_15_2023.GUI.LogInPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HCI_Tim_15_2023.GUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LogInPage">

    <Grid>
        <StackPanel>
            <Button Click="LoginAgent">Login Agent</Button>
            <Button Click="LoginClient">Login Client</Button>
            <TextBox TextWrapping="Wrap" Text="Username"/>
            <TextBox TextWrapping="Wrap" Text="Password"/>
            <Button Click="LoginClient">Login</Button>
            <Button Click="RegisterClient">Create a new account!</Button>
        </StackPanel>
    </Grid>
</Page>
